{
  "name": "mobilif-backend",
  "version": "1.0.0",
  "description": "MobiLiF - 移动健身游戏化社交平台后端服务",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:api": "node scripts/test-remote-api.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/config/database.config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/config/database.config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/database.config.ts",
    "seed": "ts-node src/database/seeds/run-seeds.ts",
    "server:status": "node scripts/server-status.js",
    "server:restart": "node scripts/server-restart.js",
    "server:backup": "node scripts/server-backup.js",
    "server:logs": "node scripts/server-logs.js",
    "server:deploy": "bash scripts/deployment/deploy.sh",
    "quick-test": "node scripts/quick-test.js",
    "setup": "node scripts/setup.js",
    "verify": "node scripts/verify-setup.js",
    "validate": "node scripts/comprehensive-validation.js",
    "validate:debug": "node scripts/comprehensive-validation.js --debug",
    "quick-validate": "node scripts/quick-validate.js",
    "examples": "node examples/usage-examples.js",
    "examples:api": "node examples/usage-examples.js api-client",
    "examples:server": "node examples/usage-examples.js server-manager",
    "examples:config": "node examples/usage-examples.js config",
    "examples:error": "node examples/usage-examples.js error-handling",
    "examples:integrated": "node examples/usage-examples.js integrated",
    "test:vscode-tasks": "node scripts/test-vscode-tasks.js",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:performance": "node scripts/test-performance.js",
    "test:db-connection": "node scripts/test-db-connection.js",
    "test:runner": "node scripts/automation/test-runner.js",
    "test:all": "node scripts/automation/test-runner.js",
    "test:all:fail-fast": "node scripts/automation/test-runner.js --fail-fast",
    "test:unit-only": "node scripts/automation/test-runner.js --skip=integration,api,e2e,performance",
    "workflow:dev": "node scripts/workflow/dev-workflow.js",
    "workflow:test": "node scripts/automation/test-runner.js",
    "workflow:miniprogram": "node scripts/workflow/miniprogram-sync.js",
    "workflow:deploy": "node scripts/workflow/deploy-workflow.js",
    "miniprogram:preview": "node scripts/miniprogram-ci.js preview",
    "miniprogram:upload": "node scripts/miniprogram-ci.js upload",
    "miniprogram:check-ip": "node scripts/miniprogram-ci.js check-ip",
    "mp:preview": "node scripts/miniprogram-ci.js preview",
    "mp:upload": "node scripts/miniprogram-ci.js upload",
    "mp:check": "node scripts/miniprogram-ci.js check-ip",
    "mp:setup": "node scripts/setup-miniprogram.js",
    "mp:help": "node scripts/miniprogram-ci.js help",
    "mp:fix": "node scripts/fix-wechat-ci.js",
    "mp:diagnose": "node scripts/diagnose-network.js",
    "mp:ipv4": "NODE_OPTIONS='--dns-result-order=ipv4first' node scripts/ipv4-wechat-ci.js preview",
    "mp:ipv4-setup": "node scripts/force-ipv4-wechat.js"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/microservices": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/schedule": "^3.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@prisma/client": "^6.12.0",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.2.0",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.9.0",
    "helmet": "^7.0.0",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma": "^6.12.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.8",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/qrcode": "^1.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "miniprogram-ci": "^2.1.14",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "MobiLiF Team",
  "license": "MIT",
  "keywords": [
    "fitness",
    "crossfit",
    "gamification",
    "social",
    "mobile",
    "nestjs",
    "typescript"
  ]
}
