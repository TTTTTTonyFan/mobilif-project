  "validationRules": {
    "gym": {
      "id": {
        "type": "string",
        "required": true
      },
      "name": {
        "type": "string",
        "required": true,
        "minLength": 1
      },
      "address": {
        "type": "string", 
        "required": true
      },
      "city": {
        "type": "string",
        "required": true
      },
      "lat": {
        "type": "number",
        "min": -90,
        "max": 90
      },
      "lng": {
        "type": "number",
        "min": -180,
        "max": 180
      },
      "rating": {
        "type": "number",
        "min": 0,
        "max": 5
      },
      "reviewCount": {
        "type": "number",
        "min": 0
      },
      "businessStatus": {
        "type": "string",
        "enum": ["营业中", "休息中", "歇业", "未知"]
      },
      "gymType": {
        "type": "string",
        "enum": ["crossfit_certified", "comprehensive", "specialty"]
      },
      "crossfitCertified": {
        "type": "boolean"
      }
    },
    "pagination": {
      "page": {
        "type": "number",
        "min": 1
      },
      "pageSize": {
        "type": "number",
        "min": 1,
        "max": 100
      },
      "total": {
        "type": "number",
        "min": 0
      }
    }
  }
}