  "expectedResponses": {
    "successResponse": {
      "code": 0,
      "message": "success",
      "data": {
        "list": [
          {
            "id": "string",
            "name": "string",
            "address": "string", 
            "city": "string",
            "district": "string",
            "lat": "number",
